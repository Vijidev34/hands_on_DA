        "import numpy as np\n",
        "\n",
        "celsius=np.array([34,30,28,35,33,29])\n",
        "F=(celsius*9/5)+32\n",
        "print(\"{:<10} {:<10}\".format(\"Celsius\",\"Farenheit\"))\n",
        "for i in range(len(celsius)):\n",
        "    print(f\"{celsius[i]:<10} {F[i]:<10}\")"
